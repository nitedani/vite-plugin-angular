{
  "name": "@nitedani/angular-renderer-core",
  "version": "17.0.0",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "angular",
    "vite"
  ],
  "repository": {
    "type": "github",
    "url": "https://github.com/nitedani/vite-plugin-angular"
  },
  "scripts": {
    "build": "rimraf lib && tsc -b && node scripts/compile-angular"
  },
  "exports": {
    ".": "./lib/index.js",
    "./server": "./lib/server/index.js",
    "./client": "./lib/client/index.js"
  },
  "typesVersions": {
    "*": {
      "server": [
        "lib/server/index.d.ts"
      ],
      "client": [
        "lib/client/index.d.ts"
      ],
      "*": [
        "lib/index.d.ts"
      ]
    }
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-server": "^17.0.0",
    "@types/node": "^18.17.15",
    "defu": "^6.1.2",
    "rimraf": "^3.0.2",
    "typescript": "^5.2.0",
    "zone.js": "^0.13.3"
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  }
}
