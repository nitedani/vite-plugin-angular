{
  "name": "@nitedani/vite-plugin-angular",
  "version": "14.3.8",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "angular",
    "vite"
  ],
  "repository": {
    "type": "github",
    "url": "https://github.com/nitedani/vite-plugin-angular"
  },
  "scripts": {
    "build": "rimraf lib && tsc -b"
  },
  "exports": {
    "./plugin": {
      "node": "./lib/plugin/index.js"
    },
    "./server": {
      "node": "./lib/server/index.js",
      "default": "./lib/server/index.js",
      "import": "./lib/server/index.js"
    },
    "./client": {
      "default": "./lib/client/index.js",
      "import": "./lib/client/index.js",
      "browser": "./lib/client/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "plugin": [
        "lib/plugin/index.d.ts"
      ],
      "server": [
        "lib/server/index.d.ts"
      ],
      "client": [
        "lib/client/index.d.ts"
      ]
    }
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@angular/cli": "^14.2.2",
    "@angular/compiler": "^14.0.0",
    "@angular/compiler-cli": "^14.2.1",
    "@swc/core": "^1.2.249",
    "defu": "^6.1.0",
    "swc-ast-helpers": "^0.2.2",
    "vite": "^3.1.0",
    "zone.js": "^0.11.8",
    "@angular-devkit/build-angular": "^14.2.3",
    "@babel/core": "^7.19.1",
    "@ngtools/webpack": "^14.2.3",
    "@nitedani/angular-renderer-core": "*"
  },
  "devDependencies": {
    "@angular/core": "^14.2.1",
    "@angular/platform-browser": "^14.2.1",
    "@angular/platform-server": "^14.2.1",
    "@types/node": "^18.7.16",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.3"
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  }
}
