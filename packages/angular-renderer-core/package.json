{
  "name": "@vikejs/angular-renderer-core",
  "version": "17.0.11",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "angular",
    "vite"
  ],
  "repository": {
    "type": "github",
    "url": "https://github.com/vikejs/vite-plugin-angular"
  },
  "scripts": {
    "build": "rimraf lib && tsc -b && node scripts/compile-angular"
  },
  "exports": {
    ".": "./lib/index.js",
    "./server": "./lib/server/index.js",
    "./client": "./lib/client/index.js"
  },
  "typesVersions": {
    "*": {
      "server": [
        "lib/server/index.d.ts"
      ],
      "client": [
        "lib/client/index.d.ts"
      ],
      "*": [
        "lib/index.d.ts"
      ]
    }
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "rxjs": "^7.8.1"
  },
  "peerDependencies": {
    "@angular/core": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-server": "^17.0.0",
    "zone.js": "*",
    "vite": "^5.0.10"
  },
  "devDependencies": {
    "@angular/compiler": "^17.0.0",
    "@angular/compiler-cli": "^17.0.8",
    "@angular/core": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-server": "^17.0.0",
    "@types/node": "^18.17.15",
    "rimraf": "^3.0.2",
    "typescript": "5.2.2",
    "zone.js": "^0.14.2",
    "vite": "^5.0.10"
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  }
}