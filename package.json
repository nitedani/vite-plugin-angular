{
  "name": "@vikejs/vite-plugin-angular",
  "version": "17.0.12",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build --concurrency=1",
    "release": "pnpm build && pnpm test && bumpp package.json packages/*/package.json examples/*/package.json && pnpm -r publish --access=public",
    "test": "playwright test"
  },
  "devDependencies": {
    "@playwright/test": "1.31.2",
    "bumpp": "^8.2.1",
    "fs-extra": "^10.1.0",
    "kill-port": "^2.0.1",
    "playwright": "1.31.2",
    "prettier": "^3.0.3",
    "turbo": "^1.10.13"
  },
  "packageManager": "pnpm@8.6.12",
  "engines": {},
  "pnpm": {
    "overrides": {
      "@vikejs/vite-plugin-angular": "link:./packages/vite-plugin-angular",
      "@vikejs/angular-renderer-core": "link:./packages/angular-renderer-core"
    }
  }
}
